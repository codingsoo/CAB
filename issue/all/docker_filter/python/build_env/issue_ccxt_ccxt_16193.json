  "dockerfile": "FROM python:3.9-slim\n\n# Install system dependencies including git for cloning the repository\nRUN apt-get update && apt-get install -y git\n\n# Clone the CCXT repository and checkout the specific commit with the fix\nRUN git clone https://github.com/ccxt/ccxt.git /ccxt\nWORKDIR /ccxt\nRUN git checkout 3b812f935ec3d977716462d0729306ba8140cab1\n\n# Install Python package dependencies from the checked-out version\nWORKDIR /ccxt/python\nRUN pip install .\n\n# Install additional runtime dependencies for async/WebSocket functionality\nRUN pip install aiohttp uvloop\n\n# Create working directory for user's validation code\nWORKDIR /app\n\n# User can mount their test script to /app and run with:\n# docker run -it --rm -v /path/to/test.py:/app/test.py <image> python test.py",
