  "dockerfile": "FROM mono:6.12.0.182-slim\n\n# Install base dependencies for .NET development and git\nRUN apt-get update && \\\n    apt-get install -y --no-install-recommends \\\n    git \\\n    mono-devel \\\n    ca-certificates-mono \\\n    referenceassemblies-pcl \\\n    nuget \\\n    curl \\\n    && rm -rf /var/lib/apt/lists/*\n\n# Clone repository and checkout specific commit\nRUN git clone https://github.com/aspnetboilerplate/aspnetboilerplate.git /src\nWORKDIR /src\nRUN git checkout f0f772cf99a18fb04fee4e5ad5c9d1c4fe5318ea\n\n# Install required NuGet version for legacy projects\nRUN mkdir -p src/.nuget && \\\n    curl -L https://dist.nuget.org/win-x86-commandline/v2.8.6/nuget.exe -o src/.nuget/NuGet.exe && \\\n    chmod +x src/.nuget/NuGet.exe\n\n# Restore NuGet packages for the solution\nRUN mono src/.nuget/NuGet.exe restore src/Abp.sln\n\n# Build the solution using xbuild with Release configuration\nRUN xbuild /p:Configuration=Release src/Abp.sln",
